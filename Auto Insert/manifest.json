{
  "manifest_version": 2,
  "name": "Auto Insert",
  "description": "フォームを自動で入力します。",
  "version": "1.0",

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["jquery-3.3.1.js", "content.js"],
          "all_frames": true
      }
  ],
    "browser_action": {
    "default_icon": {
      "16": "icon16.png"
    }
  }

}